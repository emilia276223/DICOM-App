<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewmodel="using:DICOMApp.ViewModel"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 xmlns:vm="using:DICOMApp.ViewModel"
			 xmlns:views="clr-namespace:DICOMApp"
			 x:DataType="vm:XPointLineViewModel"
             x:Class="DICOMApp.XLineView">
  <Canvas
		Width="{Binding CanvasWidth}"
		Height="{Binding CanvasHeight}"
		>
		<views:XPointView DataContext="{Binding Start}"/>
		<views:XPointView DataContext="{Binding End}"/>
		
	</Canvas>
</UserControl>
